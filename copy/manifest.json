{
  "name": "Visual Inspector(前端重构+视觉走查辅助工具)",
  "short_name": "VI",
  "version": "1.5.1",
  "manifest_version": 2,
  "default_locale": "zh_CN",
  "author": "paul",
  "description": "__MSG_pluginDesc__",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon_gray.png",
    "default_title": "Visual Inspector is off"
  },
  "permissions": [
    "unlimitedStorage",
    "storage",
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "index.css"
  ]
}