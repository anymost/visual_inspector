{
  "name": "FeHelper",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "",
  // "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["vue.min.js", "content.js"],
      "run_at": "document_end"
    }
  ]
  // "web_accessible_resources": [
  //   "inject.js"
  // ],
  // "background":{
  //   "scripts":[
  //     "background.js"
  //   ],
  // "persistent": false
  // }
}